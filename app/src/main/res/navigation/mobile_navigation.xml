<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_overview">

    <fragment
        android:id="@+id/navigation_console"
        android:name="com.emwaver.ismwaver.ui.console.ConsoleFragment"
        android:label="Console"
        tools:layout="@layout/fragment_console" />

    <fragment
        android:id="@+id/navigation_packetmode"
        android:name="com.emwaver.ismwaver.ui.packetmode.PacketModeFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_packet_mode" />

    <fragment
        android:id="@+id/navigation_overview"
        android:name="com.emwaver.ismwaver.ui.overview.OverviewFragment"
        android:label="Overview"
        tools:layout="@layout/fragment_overview" />

    <fragment
        android:id="@+id/navigation_rawmode"
        android:name="com.emwaver.ismwaver.ui.rawmode.RawModeFragment"
        android:label="Raw Mode"
        tools:layout="@layout/fragment_raw_mode" />

    <fragment
        android:id="@+id/navigation_analysis"
        android:name="com.emwaver.ismwaver.ui.analysis.AnalysisFragment"
        android:label="Analysis"
        tools:layout="@layout/fragment_analysis" />


    <!--
